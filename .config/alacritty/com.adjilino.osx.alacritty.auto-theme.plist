<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC -//Apple Computer//DTD PLIST 1.0//EN http://www.apple.com/DTDs/PropertyList-1.0.dtd >
<plist version="1.0">
    <dict>
        <key>CFBundleDisplayName</key>
        <string>Adjilino Alacritty auto-theme</string>
        <key>Label</key>
        <string>com.adjilino.osx.alacritty.auto-theme</string>
        <key>ProgramArguments</key>
        <array>
        <string>/opt/homebrew/bin/python3</string>
        <string>/Users/adjilino/.config/alacritty/auto-theme.py</string>
        </array>
        <key>KeepAlive</key>
        <true/>
        <key>StandardErrorPath</key>
        <string>/tmp/auto-theme.job.err</string>
        <key>StandardOutPath</key>
        <string>/tmp/auto-theme.job.out</string> 
    </dict>
</plist>